Algoritmo "senha_fixa"

Var
   senha, senhaDig : inteiro


Inicio
      senha <- 2002
      escreva("Digite a senha: ")
      leia(senhaDig)
      enquanto senhaDig <> senha faca
               escreva("Senha Invalida! Tente novamente: ")
               leia (senhaDig)
      fimenquanto
      escreval("Acesso permitido!")


Fimalgoritmo